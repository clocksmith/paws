{
  "name": "@paws/parsers",
  "version": "1.0.0",
  "description": "Shared parsing logic for PAWS bundle formats (cats/dogs)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./dogs-parser": {
      "types": "./dist/dogs-parser.d.ts",
      "default": "./dist/dogs-parser.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
