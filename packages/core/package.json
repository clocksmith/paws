{
  "name": "@paws/core",
  "version": "1.0.0",
  "description": "Shared resources for PAWS - personas, system prompts, and configs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./personas/*": "./personas/*",
    "./sys/*": "./sys/*",
    "./configs/*": "./configs/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "personas",
    "sys",
    "configs"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "paws",
    "personas",
    "system-prompts",
    "config"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
