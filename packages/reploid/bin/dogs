#!/usr/bin/env node

/**
 * PAWS dogs CLI wrapper for REPLOID.
 * Delegates to the canonical dogs implementation in js/dogs.js so REPLOID
 * benefits from verification, progress streaming, and latest UX improvements.
 */
// @sync-checksum: d01838036e0dfb79fb9707196599286472d6e47512c0564381eb7e5f2eb08f3c

(async () => {
  try {
    const { main } = require('@paws/cli-js/src/dogs');
    const code = await main();
    process.exit(code);
  } catch (error) {
    console.error(error instanceof Error ? error.message : error);
    process.exit(1);
  }
})();
