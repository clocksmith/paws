{
  "name": "@paws/reploid",
  "version": "0.1.0",
  "description": "REPLOID - Browser-native visual interface for PAWS with multi-agent workflows",
  "main": "boot.js",
  "type": "commonjs",
  "bin": {
    "reploid": "./bin/reploid-cli.js",
    "reploid-config": "./bin/reploid-config"
  },
  "scripts": {
    "start": "node server/proxy.js",
    "cli": "node bin/reploid-cli.js",
    "dev": "concurrently \"npm run dev:proxy\" \"npm run dev:hermes\" \"npm run dev:browser\"",
    "dev:proxy": "nodemon server/proxy.js",
    "dev:hermes": "cd hermes && npm run dev",
    "dev:browser": "python3 -m http.server 8080 || python -m http.server 8080",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui"
  },
  "keywords": [
    "reploid",
    "paws",
    "browser",
    "ai",
    "multi-agent",
    "webrtc",
    "rsi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@paws/cli-js": "workspace:*",
    "@paws/core": "workspace:*",
    "express": "^4.18.2",
    "ws": "^8.18.3",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2"
  },
  "optionalDependencies": {
    "@anthropic-ai/sdk": "^0.16.1",
    "@google/generative-ai": "^0.1.3",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^8.0.0",
    "happy-dom": "^19.0.2",
    "nodemon": "^3.0.1",
    "playwright": "^1.55.1",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
