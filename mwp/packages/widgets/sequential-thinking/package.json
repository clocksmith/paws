{
  "name": "@mwp/widget-sequential-thinking",
  "version": "1.0.0",
  "description": "MCP Widget for Sequential Thinking server - AI reasoning visualization",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@mwp/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^1.0.0",
    "happy-dom": "^12.10.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "mcp",
    "mcp-widget",
    "sequential-thinking",
    "ai-reasoning",
    "chain-of-thought",
    "web-component"
  ],
  "author": "MWP Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/mwp.git",
    "directory": "packages/widgets/sequential-thinking"
  }
}
