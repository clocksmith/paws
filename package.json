{
  "name": "paws",
  "version": "5.0.0",
  "description": "PAWS (Prepare Artifacts With SWAP) - Multi-agent AI development toolkit with CLI and browser interface (REPLOID)",
  "private": true,
  "type": "commonjs",
  "workspaces": [
    "packages/*",
    "integrations/*"
  ],
  "scripts": {
    "test": "pnpm -r test",
    "test:cli-js": "pnpm --filter @paws/cli-js test",
    "test:cli-py": "cd packages/cli-py && pytest",
    "test:reploid": "pnpm --filter @paws/reploid test",
    "test:reploid:watch": "pnpm --filter @paws/reploid test:watch",
    "test:reploid:ui": "pnpm --filter @paws/reploid test:ui",
    "test:reploid:coverage": "pnpm --filter @paws/reploid test:coverage",
    "test:e2e": "pnpm --filter @paws/reploid test:e2e",
    "test:e2e:headed": "pnpm --filter @paws/reploid test:e2e:headed",
    "test:e2e:ui": "pnpm --filter @paws/reploid test:e2e:ui",
    "test:all": "pnpm test && npm run test:cli-py",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format",
    "install:python": "cd packages/cli-py && pip install -e .",
    "setup": "pnpm install && npm run install:python",
    "reploid:start": "pnpm --filter @paws/reploid start",
    "reploid:cli": "pnpm --filter @paws/reploid cli",
    "reploid:dev": "pnpm --filter @paws/reploid dev",
    "check:reploid-sync": "node scripts/check-reploid-sync.js"
  },
  "keywords": [
    "paws",
    "reploid",
    "ai",
    "development",
    "cli",
    "browser",
    "automation",
    "code-generation",
    "git",
    "worktree",
    "interactive",
    "bundle",
    "multi-agent",
    "arena",
    "competitive-testing",
    "test-driven-selection",
    "rsi",
    "recursive-self-improvement",
    "mcp",
    "claude-code",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "chai": "^4.3.10",
    "concurrently": "^8.0.0",
    "eslint": "^8.54.0",
    "happy-dom": "^19.0.2",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "playwright": "^1.55.1",
    "prettier": "^3.1.0",
    "sinon": "^17.0.1",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/paws.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/paws/issues"
  },
  "homepage": "https://github.com/yourusername/paws#readme"
}
