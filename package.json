{
  "name": "paws",
  "version": "3.0.0",
  "description": "PAWS (Prepare Artifacts With SWAP) - Multi-agent AI development toolkit with CLI and browser interface (REPLOID)",
  "main": "index.js",
  "type": "commonjs",
  "bin": {
    "cats": "./js/cats.js",
    "dogs": "./js/dogs.js",
    "paws-session": "./js/paws-session.js",
    "reploid": "./reploid/bin/reploid-cli.js",
    "reploid-config": "./reploid/bin/reploid-config"
  },
  "scripts": {
    "test": "mocha js/test/test_paws.js",
    "test:python": "python -m pytest py/tests/",
    "test:reploid": "cd reploid && vitest run",
    "test:reploid:watch": "cd reploid && vitest",
    "test:reploid:ui": "cd reploid && vitest --ui",
    "test:reploid:coverage": "cd reploid && vitest run --coverage",
    "test:e2e": "cd reploid && playwright test",
    "test:e2e:headed": "cd reploid && playwright test --headed",
    "test:e2e:ui": "cd reploid && playwright test --ui",
    "test:all": "npm test && npm run test:python && npm run test:reploid && npm run test:e2e",
    "lint": "eslint js/*.js",
    "format": "prettier --write js/*.js",
    "install:python": "pip install -r requirements.txt",
    "setup": "npm install && npm run install:python && cd reploid/hermes && npm install",
    "reploid:start": "cd reploid && node server/proxy.js",
    "reploid:cli": "cd reploid && node bin/reploid-cli.js",
    "reploid:dev": "cd reploid && concurrently \"npm run dev:proxy\" \"npm run dev:hermes\" \"npm run dev:browser\"",
    "reploid:dev:proxy": "cd reploid && nodemon server/proxy.js",
    "reploid:dev:hermes": "cd reploid/hermes && npm run dev",
    "reploid:dev:browser": "python3 -m http.server 8080 --directory reploid || python -m http.server 8080 --directory reploid",
    "hermes": "cd reploid/hermes && node index.js"
  },
  "keywords": [
    "paws",
    "reploid",
    "ai",
    "development",
    "cli",
    "browser",
    "automation",
    "code-generation",
    "git",
    "worktree",
    "interactive",
    "bundle",
    "multi-agent",
    "paxos",
    "rsi",
    "recursive-self-improvement",
    "mcp",
    "claude-code",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "acorn": "^8.12.1",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^4.1.2",
    "chart.js": "^4.5.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "d3": "^7.9.0",
    "diff": "^5.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.3.1",
    "glob": "^10.3.10",
    "ignore": "^5.3.0",
    "inquirer": "^8.2.6",
    "node-fetch": "^3.3.2",
    "ora": "^5.4.1",
    "prismjs": "^1.29.0",
    "simple-git": "^3.20.0",
    "uuid": "^9.0.1",
    "ws": "^8.18.3",
    "yargs": "^18.0.0",
    "zod": "^4.1.5"
  },
  "optionalDependencies": {
    "@anthropic-ai/sdk": "^0.16.1",
    "@google/generative-ai": "^0.1.3",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "chai": "^4.3.10",
    "concurrently": "^8.0.0",
    "eslint": "^8.54.0",
    "happy-dom": "^19.0.2",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "playwright": "^1.55.1",
    "prettier": "^3.1.0",
    "sinon": "^17.0.1",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/paws.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/paws/issues"
  },
  "homepage": "https://github.com/yourusername/paws#readme"
}
